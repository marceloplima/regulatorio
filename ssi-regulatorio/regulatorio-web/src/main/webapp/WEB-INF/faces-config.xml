<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>cadssi</from-outcome>
			<to-view-id>/interno/cadssi.xhtml</to-view-id>
		</navigation-case>		
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/index_sistema.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>cadssi</from-outcome>
			<to-view-id>/interno/cadssi.xhtml</to-view-id>
		</navigation-case>		
	</navigation-rule>
	
    <application>   
	    <!-- Arquivo properties com as mensagens do sistema -->   
	    <message-bundle>mensagens</message-bundle>
	    <locale-config>
	    	<default-locale>pt_BR</default-locale>
	    </locale-config>   
	</application>

</faces-config>
